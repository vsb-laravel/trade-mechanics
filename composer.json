{
    "name": "vsb-laravel/trade-mechanics",
    "type":"composer-plugin-laravel",
    "description": "Laravel trade-mechanics model",
    "keywords": ["trade-mechanics", "laravel", "vsb"],
    "homepage": "https://github.com/vsb-laravel/trade-mechanics",
    "support": {
        "issues": "https://github.com/vsb-laravel/trade-mechanics/issues",
        "source": "https://github.com/vsb-laravel/trade-mechanics"
    },
    "authors": [
        {
            "name": "Vladimir Bushuev",
            "email": "v.bushuev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "laravel/framework": "5.8.x"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Vsb\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/fake.php"
        ],
        "psr-4": {
            "Vsb\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        },
        "laravel": {
            "providers": [
                "Vsb\\TradeMechanicsServiceProvider"
            ]
        }
    }
}
